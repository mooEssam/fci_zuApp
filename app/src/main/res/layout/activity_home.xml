<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawerhome"
    android:fitsSystemWindows="true"
    tools:openDrawer="start"
    tools:context=".Home">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/memobaar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#614D85"
            android:elevation="5dp"
            style="@style/Theme.AppCompat.NoActionBar"
            />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/containerfrag"
            />

    </LinearLayout>
    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:id="@+id/nav_view"
        app:headerLayout="@layout/nav_header_nave"
        app:menu="@menu/menue_nav"
        app:itemTextColor="#645C73"
        app:itemIconTint="#350B83"
        android:background="#FFFFFF"
        />

</androidx.drawerlayout.widget.DrawerLayout>